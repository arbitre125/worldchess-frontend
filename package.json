{
  "name": "worldchess-fe",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:stage": "ng serve --configuration=devstage",
    "start:docker": "ng serve --host 0.0.0.0",
    "start:aot": "ng serve --aot",
    "build": "per-env",
    "build:dev": "ng build --configuration=dev",
    "build:devstage": "ng build --configuration=devstage",
    "build:stage": "ng build --configuration=stage",
    "build:prod": "ng build --prod --configuration=production",
    "start:widget": "ng serve --project worldchess-widget",
    "build:widget": "per-env",
    "build:widget:dev": "ng build  --project worldchess-widget --configuration=dev",
    "build:widget:stage": "ng build  --project worldchess-widget --configuration=stage",
    "build:widget:prod": "ng build --project worldchess-widget --prod --configuration=production",
    "widget:start": "ng serve --project widget",
    "widget:build": "per-env",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "doc:build": "compodoc -p src/tsconfig.app.json",
    "doc:serve": "compodoc -s",
    "doc:buildandserve": "compodoc -p src/tsconfig.app.json -s",
    "bundle-report": "ng build --prod --configuration=production --stats-json && webpack-bundle-analyzer dist/stats.json"
  },
  "browserslist": [
    "Edge >= 15",
    "Explorer >= 10",
    "Chrome >= 41",
    "Firefox >= 52",
    "Safari >= 10",
    "ChromeAndroid >= 60",
    "iOS >= 10"
  ],
